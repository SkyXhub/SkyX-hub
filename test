    Color3.fromRGB = function(r, g, b)
        return {r = r/255, g = g/255, b = b/255}
    end
    
    -- Mock workspace
    workspace = {
        GetChildren = function()
            return {}
        end,
        Enemies = {
            GetChildren = function()
